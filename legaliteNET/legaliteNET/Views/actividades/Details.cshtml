@model legaliteNET.Models.actividade

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>


</head>
<body>
    <section id="content" class="table-layout animated fadeIn">


        <div class="chute chute-center">
            <div class="row">
                <div class="col-xs-12">
                    <div class="panel">




                        <div class="panel-heading">
                            <span class="panel-title hidden-xs">DETALLE DE LA ACTIVIDAD</span>
                        </div>
                        <div class="panel-body pn">









                            <dl class="dl-horizontal">

                                <dt>
                                   Id
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.id)
                                </dd>
                                <dt>
                                    Actividad
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.actividad)
                                </dd>

                                <dt>
                                    Valor
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Valor)
                                </dd>

                               

                            

                            </dl>


                            <p>


                                <button type="submit" class="btn btn-bordered btn-primary pull-right">@Html.ActionLink("Salir ", "Index")</button>

                            </p>

                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>



</body>
</html>